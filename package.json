{
  "name": "promdash",
  "version": "1.1.0",
  "description": "ES6 implementation of lodash",
  "main": "promdash.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/johngeorgewright/promdash"
  },
  "scripts": {
    "build": "babel -o promdash.js promdash.es",
    "link-jspm": "nodemon -x \"npm run build && jspm link npm:promdash -y\" -e es",
    "prepublish": "npm test && npm run build",
    "test": "mocha --check-leaks --compilers es:babel/register",
    "test-watch": "mocha --watch --growl --check-leaks --compilers es:babel/register"
  },
  "keywords": [
    "Promise",
    "lodash"
  ],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.1.13",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.0.0",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0"
  },
  "dependencies": {
    "lodash": "*"
  }
}
